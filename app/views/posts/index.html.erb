<h1>Time Entries</h1>
<table class="table table-hover table-striped">
<thead>
  <tr>
    <th>
     Log #
    </th>
    <th>
      Overtime Request
    </th>
    <th>
      Date
    </th>
    <th>
      Employee Name
    </th>
    <th>
      Rationale
    </th>
    <th>
      Status
    </th>
  </tr>
</thead>
<tbody id="posts">
  <%= render @posts %>
</tbody>
</table>

<div id="paginator">
  <%= paginate @posts, remote: true %>
</div>